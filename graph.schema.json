{
  "$ref": "#/definitions/Graph",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Graph": {
      "additionalProperties": false,
      "properties": {
        "links": {
          "items": {
            "$ref": "#/definitions/Link"
          },
          "type": "array"
        },
        "nodes": {
          "items": {
            "$ref": "#/definitions/Node"
          },
          "type": "array"
        }
      },
      "required": ["nodes", "links"],
      "type": "object"
    },
    "Link": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "source": {
          "type": "string"
        },
        "target": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/LinkType"
        },
        "value": {
          "type": "number"
        },
        "weights": {
          "additionalProperties": {
            "type": "number"
          },
          "type": "object"
        }
      },
      "required": ["source", "target"],
      "type": "object"
    },
    "LinkType": {
      "enum": [
        "to",
        "from",
        "has",
        "in",
        "child",
        "implicit",
        "implicitFrom",
        "implicitAlias"
      ],
      "type": "string"
    },
    "Node": {
      "additionalProperties": false,
      "properties": {
        "aliases": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "value": {
          "type": "number"
        },
        "weights": {
          "additionalProperties": {
            "type": "number"
          },
          "type": "object"
        }
      },
      "required": ["id"],
      "type": "object"
    }
  }
}
