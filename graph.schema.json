{
  "$ref": "#/definitions/Graph",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Graph": {
      "additionalProperties": false,
      "properties": {
        "links": {
          "items": {
            "$ref": "#/definitions/GraphLink"
          },
          "type": "array"
        },
        "nodes": {
          "items": {
            "$ref": "#/definitions/GraphNode"
          },
          "type": "array"
        }
      },
      "required": ["nodes", "links"],
      "type": "object"
    },
    "GraphLink": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "source": {
          "type": "string"
        },
        "target": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/LinkType"
        },
        "value": {
          "type": "number"
        }
      },
      "required": ["source", "target"],
      "type": "object"
    },
    "GraphNode": {
      "additionalProperties": false,
      "properties": {
        "aliases": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "value": {
          "type": "number"
        }
      },
      "required": ["id"],
      "type": "object"
    },
    "LinkType": {
      "enum": [
        "to",
        "from",
        "has",
        "in",
        "child",
        "implicit",
        "implicitFrom",
        "implicitAlias"
      ],
      "type": "string"
    }
  }
}
