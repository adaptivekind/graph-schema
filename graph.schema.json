{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Graph": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "links": {
          "items": {
            "$ref": "#/definitions/Link"
          },
          "type": "array"
        },
        "nodes": {
          "additionalProperties": {
            "$ref": "#/definitions/Node"
          },
          "type": "object"
        }
      },
      "required": ["nodes", "links"],
      "type": "object"
    },
    "Link": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "meta": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "source": {
          "type": "string"
        },
        "target": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "value": {
          "type": "number"
        },
        "weights": {
          "additionalProperties": {
            "type": "number"
          },
          "type": "object"
        }
      },
      "required": ["source", "target"],
      "type": "object"
    },
    "Node": {
      "additionalProperties": false,
      "properties": {
        "aliases": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "label": {
          "type": "string"
        },
        "meta": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "type": {
          "type": "string"
        },
        "value": {
          "type": "number"
        },
        "weights": {
          "additionalProperties": {
            "type": "number"
          },
          "type": "object"
        }
      },
      "type": "object"
    }
  }
}
